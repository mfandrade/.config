theme = dark:GitHub-Dark-High-Contrast,light:GitHub-Light-High-Contrast
font-family = "JetBrainsMono Nerd Font"
font-family-bold = "JetBrains Mono Nerd Font
font-family-italic = "JetBrains Mono Nerd Font Italic"
font-family-bold-italic = "JetBrains Mono Nerd Font Italic"
bold-is-bright = false
background-opacity = 0.50
cursor-style = block
cursor-style-blink = true
cursor-invert-fg-bg = true
shell-integration = none
shell-integration-features = no-cursor
confirm-close-surface = false
keybind = ctrl+slash=text:/
keybind = shift+ctrl+slash=text:?
